<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Color x:Key="ThemeColor">#FF9B71</Color>
	<SolidColorBrush x:Key="ThemeColorBrush" Color="{StaticResource ThemeColor}" />

	<Color x:Key="RedColor">#990033</Color>
	<SolidColorBrush x:Key="RedBrush" Color="{StaticResource RedColor}" />

	<Color x:Key="GreenColor">#018574</Color>
	<SolidColorBrush x:Key="GreenBrush" Color="{StaticResource GreenColor}" />

	<Color x:Key="BlueColor">#0063B1</Color>
	<SolidColorBrush x:Key="BlueBrush" Color="{StaticResource BlueColor}" />
	
	<Color x:Key="WhiteColor">#f6f6f6</Color>
	<SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource WhiteColor}" />
	
	<Color x:Key="TintColor">#2B3A67</Color>
	<SolidColorBrush x:Key="TintBrush" Color="{StaticResource TintColor}" />
	
	<Style TargetType="{x:Type Window}"> 
		<Setter Property="FontFamily" Value="pack://application:,,,/Resources/Fonts/OpenSans-Regular.ttf" />             
	</Style> 
	
	<Style TargetType="{x:Type TextBlock}"> 
		<Setter Property="FontFamily" Value="pack://application:,,,/Resources/Fonts/OpenSans-Regular.ttf" />             
	</Style> 
</ResourceDictionary>