<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Color x:Key="ThemeColor">#990033</Color>
	<SolidColorBrush x:Key="ThemeColorBrush" Color="{StaticResource ThemeColor}" />

	<Color x:Key="RedColor">#990033</Color>
	<SolidColorBrush x:Key="RedBrush" Color="{StaticResource RedColor}" />

	<Color x:Key="GreenColor">#018574</Color>
	<SolidColorBrush x:Key="GreenBrush" Color="{StaticResource GreenColor}" />

	<Color x:Key="BlueColor">#0063B1</Color>
	<SolidColorBrush x:Key="BlueBrush" Color="{StaticResource BlueColor}" />
</ResourceDictionary>